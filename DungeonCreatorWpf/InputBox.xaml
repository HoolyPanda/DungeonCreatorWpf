<Window x:Class="DungeonCreatorWpf.InputBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DungeonCreatorWpf"
        mc:Ignorable="d"
        MinHeight="130"
        MinWidth="210"
        MaxWidth="500"
        Title="InputBox" Height="125.824" Width="152.857">
    <Grid Margin="0,0,2,5">
        <TextBox Name="Data" HorizontalAlignment="Left" Height="23" Margin="10,39,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button Name="YesButton" Click="YesButton_Click" Content="Yes" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="9,67,0,0"/>
        <Button Name="NoButton" Click="NoButton_Click" Content="No" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="101,67,0,0"/>
        <Label Name="Message" Content="Something" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0"/>
    </Grid>
</Window>
